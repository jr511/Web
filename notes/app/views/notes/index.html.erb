</main>

<footer class="text-muted">
  <div class="form-signin">
    <p class="float-right">
      <a href="#">Back to top</a>
      <% @users.each do |user| %>
  		<% if user.name == session[:user] %>
  			<% if user.admin == true %>
  				<p> Hola admin! </p>
  				<h1 class="text-dark"><strong> Notes administration panel </strong></h1>
  				<table class="table text-center">
  					<thead>
  						<tr>
  							<th> ID Nota </th>
  							<th> Titulo </th>
  							<th> Contenido </th>
  							<th> Propietario </th>
  							<th> ID Propietario </th>
  							<th colspan="3"></th>
  						</tr>
  					</thead>
  					<tbody>
  						<% @notes.each do |note| %>
  							<tr>
  								<td><%=note.id%></td>
  								<td><%=note.title%></td>
  								<td><%=note.content%></td>
  								<td><%=note.user.name%></td>
  								<td><%=note.user.id%></td>
  								<td><%= link_to "Show", note %></td>
  								
  							</tr>
  							<%end%>
  			<% end %>
  			<%end%>
  			<%end%>
    </p>
  </div>
